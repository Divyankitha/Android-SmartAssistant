<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@drawable/city">

    <TextView
        android:text="Interact"
        android:layout_height="50dp"
        android:id="@+id/textView5"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:layout_below="@+id/aboutUs"
        android:layout_alignStart="@+id/aboutUs"
        android:textAlignment="center"
        android:layout_width="80dp" />

    <TextView
        android:text="About 3D Tree"
        android:layout_width="wrap_content"
        android:id="@+id/textView4"
        android:layout_height="30dp"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textAlignment="center"
        android:layout_below="@+id/interact"
        android:layout_alignEnd="@+id/aboutUs"
        android:layout_marginTop="12dp" />

    <ImageButton
        android:src="@drawable/scanner"
        android:id="@+id/interact"
        android:scaleType="fitCenter"
        android:onClick="gotoScan"
        android:background="#ffff"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="29dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/aboutUs" />

    <ImageButton
        android:src="@drawable/touch2"
        android:id="@+id/aboutUs"
        android:scaleType="fitCenter"
        android:onClick="gotoAboutUs"
        android:background="#ffff"
        android:layout_height="80dp"
        android:layout_width="90dp"
        android:layout_marginStart="31dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:src="@drawable/camera"
        android:id="@+id/photo"
        android:scaleType="fitCenter"
        android:onClick="gotoPicture"
        android:background="#ffff"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/location"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Photo/Video"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:id="@+id/textView6"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:textSize="16dp"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignStart="@+id/photo" />

    <TextView
        android:text="Share"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/textView7"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textAlignment="center"
        android:layout_below="@+id/share"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="FeedBack"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:id="@+id/textView9"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textAlignment="center"
        android:layout_below="@+id/feedback"
        android:layout_alignStart="@+id/feedback" />

    <TextView
        android:text="NearBy Places"
        android:layout_height="50dp"
        android:id="@+id/textView8"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textAlignment="center"
        android:layout_width="100dp"
        android:layout_above="@+id/share"
        android:layout_alignEnd="@+id/textView9" />

    <ImageButton
        app:srcCompat="@drawable/share"
        android:id="@+id/share"
        android:scaleType="fitCenter"
        android:onClick="gotoShare"
        android:background="#ffff"
        android:layout_height="80dp"
        android:layout_width="90dp"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <ImageButton
        android:src="@drawable/feedback"
        android:id="@+id/feedback"
        android:scaleType="fitCenter"
        android:onClick="gotoFeedback"
        android:background="#ffff"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="30dp"
        android:layout_below="@+id/textView8"
        android:layout_toEndOf="@+id/share" />

    <ImageButton
        android:src="@drawable/locationearth"
        android:id="@+id/location"
        android:scaleType="fitCenter"
        android:background="#ffff"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:onClick="gotoLocation"
        android:layout_above="@+id/textView4"
        android:layout_alignStart="@+id/feedback" />


</RelativeLayout>